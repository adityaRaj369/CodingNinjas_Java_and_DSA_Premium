import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
            Scanner s=new Scanner(System.in);
			
			int allow;
			int basic=s.nextInt();
			String str=s.next();
			char grade=str.charAt(0);
			
			double hra =0.2 * basic;
			double da = 0.5 * basic;
			double pf=0.11*basic;
			if(grade=='A'){
				allow=1700;
			}
			if(grade=='B'){
				allow=1500;
			}
			else{
				allow=1300;
			}
			int totalsalary=basic+hra+da+allow-pf;

	}
}
