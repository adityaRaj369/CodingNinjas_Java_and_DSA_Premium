import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=1;
		int maxvalue=2*n-1;
		while (i<=n){
			int j =1;
			int odd = 2 * i - 1;
			while(j<=n){
				
				if (odd > maxvalue) {
					odd = 1;
				}
				
				System.out.print(odd);
				
				odd+=2;
				j++;
			}
			
			System.out.println();
			i++;
			
			
			
		}
	
	}
}
