public class Solution {
    public static int[] printColWise(int [][]a) {
        // Write your code here.
         int arr[]=new int[a.length*a[0].length];
		int count=0;
		
		for(int i=0;i<a[0].length;i++) {
			for(int j=0;j<a.length;j++) {
				arr[count]=a[j][i];
				count++;
			}
			
		}
		return arr;
	}
}
    
