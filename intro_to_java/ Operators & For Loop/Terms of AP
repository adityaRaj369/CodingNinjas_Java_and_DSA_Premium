import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
      Scanner s= new Scanner(System.in);
	  int n=s.nextInt();
	  int i=1;
  
	  int count=0;
	  
	  while(count<n){
		  
         int  term=3*i+2;
		  if(term%4!=0){
			  System.out.print(term+" ");
              count=count+1;
		  }
		   i++;
	  }
	}
}
