public class Solution {
    public static void trianglePattern(int n) {
        // Write your code here
        

       
        int i = 1;
        int p = 1;
        while (i <= n) {
            int j = 1;
           
            while (j <= i) {
                System.out.print(n-j+1+" ");
                j = j + 1;
               
            }
            System.out.println( );
            i = i + 1;
        }
    }
}
